.root {
    display: grid;
    justify-content: flex-start;
    align-items: stretch;
}

.column {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: stretch;

    &:last-of-type {
        margin-right: 0 !important;
    }
}

.cell {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: stretch;

    &:last-of-type {
        margin-bottom: 0 !important;
    }
}
