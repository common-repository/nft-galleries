.root {
    display: flex;
    flex-direction: column;
    align-items: stretch;
    width: 100%;
    height: 100%;
}

.navbar {
    flex: 0 0 52px;
    display: flex;
    flex-direction: row;
    align-items: stretch;
    height: 52px;
    z-index: 1;
}

.content {
    flex: 1;
    display: flex;
    flex-direction: row;
    align-items: stretch;
    position: relative;
    overflow: hidden;
    min-width: 400px;
    z-index: 0;
}
